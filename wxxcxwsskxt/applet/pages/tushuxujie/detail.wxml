            <view style="color: #999999"> {{ map.guihuanshijian }} </view>
        </van-cell>
        <van-cell icon="" arrow-direction="right">
            <view slot="title" style="color: #000000"> 续借到 </view>
            <view style="color: #999999"> {{ map.xujiedao }} </view>
        </van-cell>
    </van-cell-group>
</view>

<view style="padding-bottom: 20rpx">
    <view class="cell-title" style="padding: 8rpx; background: #FFFFFF"> 续借备注 </view>
